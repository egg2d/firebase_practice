<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navRightMain"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


        <TextView
            android:id="@+id/txtNickname0"
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:gravity="center"
            android:text="내정보_닉네임"
            android:background="@color/colorAccent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.26" />


        <TextView
            android:id="@+id/txtEmail0"
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:gravity="center"
            android:text="내정보_이메일"
            android:background="@color/colorPrimary"
            app:layout_constraintTop_toBottomOf="@+id/txtNickname0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <TextView
            android:id="@+id/txtNickname1"
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:gravity="center"
            android:text="내정보_닉네임"
            android:background="@color/colorAccent"
            app:layout_constraintTop_toBottomOf="@+id/txtEmail0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />


        <TextView
            android:id="@+id/txtEmail1"
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:gravity="center"
            android:text="내정보_이메일"
            android:background="@color/colorPrimary"
            app:layout_constraintTop_toBottomOf="@+id/txtNickname1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />




</android.support.constraint.ConstraintLayout>